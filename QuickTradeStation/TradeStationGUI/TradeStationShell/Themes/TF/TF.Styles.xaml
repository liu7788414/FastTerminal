                        <Border x:Name="hoverElem" BorderBrush="{StaticResource CellItemHoverBorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{StaticResource XamGridSelectedCellControlBackgroundBrush}" Visibility="Collapsed" Grid.ColumnSpan="2" />
